{
    "name": "carnage/jwt-zend-auth",
    "description": "JWT authentication module for ZF2",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5",
        "zendframework/zendframework": "^2.3.0",
        "lcobucci/jwt":"~3.0.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.0",
        "phpunit/phpunit": "^4.0.14"
    },
    "autoload": {
        "psr-4": {
            "Carnage\\JwtZendAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Carnage\\JwtZendAuthTest\\": "test/"
        }
    }
}
